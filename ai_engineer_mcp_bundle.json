{  "project_name": "AI_Engineer_MCP_Bundle",  "description": "Conjunto de 9 fluxos MCP para Engenheiros de IA, incluindo RAG, agentes de voz, análise financeira, geração de dados sintéticos, e integração desktop.",  "version": "1.0",  "modules": [    {      "name": "DeepResearcher",      "type": "multi-agent",      "tasks": ["web_search", "pdf_indexing", "note_taking"],      "output": "project_summary.md"    },    {      "name": "SyntheticDataGen",      "type": "generator",      "stack": ["sdv", "pandas", "numpy"],      "storage": "local_sdv.db"    },    {      "name": "LocalMCPClient",      "type": "llm-client",      "config": {        "mode": "offline",        "models": ["llama2", "phi3"],        "vectorstore": "sqlite",        "llm_host": "127.0.0.1:11434"      }    },    {      "name": "RAG_Workflow",      "type": "retriever-generator",      "source": ["docs/*.pdf"],      "llm": "ollama",      "database": "chroma"    },    {      "name": "ComplexDocsRAG",      "type": "retriever",      "input": ["contracts", "logs", "emails"],      "llm": "openrouter"    },    {      "name": "FinAI",      "type": "analyzer",      "data": "statements.csv",      "stack": ["pandas", "langchain"],      "output": "dashboard.html"    },    {      "name": "QwenVoice",      "type": "voice-agent",      "input": "mic",      "llm": "Qwen",      "transcriber": "whisper",      "executor": "shell_tasks"    },    {      "name": "MindsDBConnector",      "type": "predictive-db",      "engine": "mindsdb",      "tasks": ["forecasting", "classification"],      "frontend": "streamlit"    },    {      "name": "DesktopBridge",      "type": "integration",      "apps": ["Outlook", "Excel", "VSCode"],      "bridge_script": "email_memory_connector.py",      "memory_strategy": "local_file_system"    }  ],  "commands": [    "compile",    "deploy",    "sync-all"  ]}